<div class="control-group deform-field">
  	<label class="control-label">Address</label>
  	<div class="controls controls-row">
	  	<input class="span12 deform-out" data-deform-subfield="address" type="text" placeholder="Street Address">
  	</div>
  	<div class="controls controls-row">
	  	<input class="span12 deform-out" data-deform-subfield="address2" type="text" placeholder="Address Line 2">
  	</div>
  	<div class="controls controls-row">
	  	<input class="span6 deform-out" data-deform-subfield="city" type="text" placeholder="City">
	  	<input class="span6 deform-out" data-deform-subfield="state" type="text" placeholder="State / Province / Region">
  	</div>
  	<div class="controls controls-row">
	  	<input class="span6 deform-out" data-deform-subfield="zipcode" type="text" placeholder="Postal / ZIP Code">
  	</div>
</div>



